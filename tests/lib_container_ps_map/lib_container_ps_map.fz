lib_container_ps_map is
  a := (container.ps_map i32 String).empty
  a1 := a.add 3 "3"
  a2 := a1.add 5 "5"

  say a1
  say a2

  say "size : {a1.size}"
  say "size : {a2.size}"
  say "min  : {a1.min}"
  say "min  : {a2.min}"
  say "max  : {a1.max}"
  say "max  : {a2.max}"
  say "keys : {a1.as_key_array}"
  say "keys : {a2.as_key_array}"
  say "items: {a1.items}"
  say "items: {a2.items}"
  say "union: {a1.union a2}"
  say "union: {a2.union ((container.ps_map i32 String).from_sequence [7] ["7"])}"
